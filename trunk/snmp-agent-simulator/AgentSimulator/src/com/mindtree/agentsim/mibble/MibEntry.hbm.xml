<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.mindtree.agentsim.mibble.MibEntry" table="mibentry">
		<id name="id" column="id" type="java.lang.Long" unsaved-value="0">
           <generator class="increment"/>
        </id>
		<property name="entryName" column="entryName" type="string" />
		<property name="oid" column="oid" type="string" />
		<property name="type" column="type" type="string" />
		<property name="value" column="value" type="string" />
		<property name="access" column="access" type="string" />
		<property name="valueGenType" column="valueGenType" type="string" />
		<property name="constraints" column="constraints" type="string" />
	</class>
</hibernate-mapping>